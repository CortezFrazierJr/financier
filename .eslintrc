{
  "extends": "eslint:recommended",

  "overrides": [
    {
      "files": ["**/*.spec.js"],
      "plugins": ["jest"],
      "extends": ["plugin:jest/recommended"],
      "rules": { "jest/prefer-expect-assertions": "off" }
    }
  ],

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },

  "env": {
    "browser": true,
    "node": true
  },

  "globals": {
    "angular": true,
    "inject": true,
    "VERSION": true
  },

  "rules": {}
}
